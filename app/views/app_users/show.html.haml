%p#notice= notice

%p
  %b Device Id:
  = @app_user.device_id
%p
  %b fb Name:
  = @app_user.fb_name
%p
  %b fb Token:
  = @app_user.fb_token
%p
  %b fb User Id:
  = @app_user.fb_user_id
%p
  %b Age:
  = @app_user.age
%p
  %b Area:
  = @app_user.area #.join(', ')
%p
  %b Favorite Beverage:
  = @app_user.favorite_beverage #.join(', ')
%p
  %b Favorite Music:
  = @app_user.favorite_music.join(', ')
%p
  %b Past Attractions:
  = @app_user.past_attractions
%p
  %b Created At:
  = @app_user.created_at
%p
  %b Updated At:
  = @app_user.updated_at

= link_to 'Back', app_users_path(format: :html), class: 'btn'

